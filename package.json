{
  "name": "component-forge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run start:api & vite",
    "dev:frontend": "vite",
    "start:api": "python api/tempo.py",
    "build": "npm run clean && tsc -b && vite build",
    "build:vercel": "rm -rf .next dist build .cache && tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "analyze": "npm run build:analyze",
    "clean": "rm -rf .next dist build .cache node_modules/.cache && find . -name '__pycache__' -type d -exec rm -rf {} + 2>/dev/null || true",
    "size": "npm run build && ls -lah dist/",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.20",
    "@types/three": "^0.180.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.10",
    "google-map-react": "^2.2.5",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "2.15.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.9",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "3.5.3",
    "tailwindcss": "^4.0.9",
    "tw-animate-css": "^1.3.7",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^1.2.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
