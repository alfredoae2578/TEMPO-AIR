{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.12",
      "excludeFiles": "{.next,*.cache,node_modules,public,app,dist,build,src,components,EXTERNALPROYECTS}/**",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/tempo",
      "destination": "/api/tempo.py"
    }
  ]
}